{
  "$schema": "http://json-schema.org/schema",
  "cli": "nx",
  "$id": "ui-lib-component",
  "type": "object",
  "properties": {
    "directory": {
      "type": "string",
      "description": "path to directory containing the component",
      "$default": {
        "$source": "argv",
        "index": 0
      }
    },
    "componentName": {
      "type": "string",
      "description": "component name",
      "$default": {
        "$source": "argv",
        "index": 1
      }
    }
  },
  "required": ["directory", "componentName"]
}
