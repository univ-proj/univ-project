{
  "extends": ["../../.eslintrc.json"],
  "ignorePatterns": ["!**/*"],
  "overrides": [
    {
      "files": ["*.ts", "*.tsx", "*.js", "*.jsx"],
      "rules": {}
    },
    {
      "files": ["*.ts", "*.tsx"],
      "rules": {}
    },
    {
      "files": ["*.js", "*.jsx"],
      "rules": {
        "no-console": "warn",
        "no-unused-vars": ["error", { "ignoreRestSiblings": true }],
        "consistent-return": "warn"
      }
    },
    {
      "files": "*.spec.js",
      "plugins": ["jest"]
    }
  ]
}
